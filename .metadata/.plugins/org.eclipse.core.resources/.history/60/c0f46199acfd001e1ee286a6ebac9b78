import java.awt.Graphics2D;
import java.awt.Rectangle;

import javax.swing.JPanel;

public class legoPiece extends JPanel{
	private static final long serialVersionUID = 1L;
	private int x, y, width, height;
	
	
	public void drawLego(Graphics2D g) {
		super.paintComponent(g);
		Rectangle r = new Rectangle(x, y, width, height);
		g.fillOval(x, y, width/5, height/5);
		g.fillOval(x+(width/2), y, width/5, height/5);
		g.fillOval(x, y+(height/2), width/5, height/5);
		g.fillOval(x+(width/2), y+(height/2), width/5, height/5);
	}
}
