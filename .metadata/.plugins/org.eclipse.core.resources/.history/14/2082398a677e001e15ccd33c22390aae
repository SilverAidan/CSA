
public class Words {
	String[] words;

	public Words(String[] words) {
		this.words = words;
	}

	public String[] getWords() {
		return words;
	}

	public void setWords(String[] words) {
		this.words = words;
	}
	public int indexOf(String word) {
		for(int i = 0; i < words.length; i++) {
			if (words[i].equals(word)) {
				return i;
			}
		}
		return -1;
	}
}
